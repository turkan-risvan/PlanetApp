        android:label="@string/app_name"
        android:roundIcon="@drawable/saturn"
        android:supportsRtl="true"
        android:theme="@style/Theme.Planeto"
        tools:targetApi="31">
        <activity
            android:name=".SplashScreen"
            android:screenOrientation="portrait" />
        <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
    </intent-filter>
        <activity
            android:name=".finalActivity"  android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name=".PlanetDetails"  android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name=".MainActivity"  android:screenOrientation="portrait"
            android:exported="true">

        </activity>
    </application>

</manifest>